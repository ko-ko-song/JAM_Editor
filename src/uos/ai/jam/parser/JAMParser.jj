   		GoalAction goalAction = null;
		try {
			goalAction = parser.goal_action(plan.getSymbolTable(), interpreter);
		} catch (ParseException e) {
			// TODO Auto-generated catch block
			// e.printStackTrace();
		} 
		return goalAction;
	}

	public static Map<String, String> parsePrefixes(String string) {
		JAMParser parser;
		parser = new JAMParser(new ByteArrayInputStream(string.getBytes()));
		Interpreter interpreter = new Interpreter();
		   try {
					interpreter = (Interpreter)parser.parsePrefixes(interpreter);
				} catch (ParseException e) {
					// TODO Auto-generated catch block
					return null;
				}
						
		List<Prefix> prefixes =  interpreter.getNameSpaceTable().getPrefixes();
		Map<String, String> prefixMap = new HashMap<String, String>();
		for(Prefix prefix : prefixes) {
			prefixMap.put(prefix.getPrefix(), prefix.getValue());
		}

		return prefixMap;
	}



	
