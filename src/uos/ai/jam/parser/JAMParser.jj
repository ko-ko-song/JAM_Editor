{}
{ 
	plan_id(currentPlan, interpreter)
| 	plan_name(currentPlan, interpreter)
|	plan_doc(currentPlan)
//|	plan_goalspec(currentPlan, interpreter)
//|	plan_concludespec(currentPlan, interpreter)
|	plan_context(currentPlan, interpreter)
