Relation relation(SymbolTable symbolTable, Interpreter interpreter) :
{
	Token id = null;
	Token name_space = null;
	Prefix prefix = null;
	Token prefi = null;
	Token[] tokens;
	List<Expression> el = null;
}
{	  prefi = <IDENTIFIER> (":" id = <IDENTIFIER>)* [ el=object_arguments(symbolTable, interpreter) ]
		{
		  if(id!=null) {
		    //editor
		    	String relation_name;
		    	if(interpreter.getNameSpaceTable().getNameSpace(prefi.image) != null) {
					relation_name = interpreter.getNameSpaceTable().getNameSpace(prefi.image).wrap(id.image);
		    	}
				else {
				  relation_name = prefi.image + ":" + id.image;
				}
		    	
		    	//editor
		    	Relation r = interpreter.getWorldModel().newRelation(relation_name, el);
		    	r.set_fileName(this.fileName);
		    	r.set_line(prefi.beginLine);
		    	r.set_beginColumn(prefi.beginColumn);
	  			return r;
	  		}else {
	  		  	id = prefi;
	  		  	//editor
				Relation r = interpreter.getWorldModel().newRelation(id.image, el);
		    	r.set_fileName(this.fileName);
		    	r.set_line(prefi.beginLine);
		    	r.set_beginColumn(prefi.beginColumn);
				return r;
			} 
		} 
	| tokens = owl() [ el=object_arguments(symbolTable, interpreter) ] 
		{
		  	id = tokens[1];
		  	name_space = tokens[0];
			return interpreter.getWorldModel().newRelation(id.image, el);  /// @need to be modified.
		}
}


Token[] owl():
{
  	Token[] owl_token = {null,null,null};
  	Token protocol_type = null;
	Token id = null;
	Token name_space = null;
}
{
	"<" protocol_type = <PROTOCOLTYPE> name_space = namespace() (id = <CLASS_IDENTIFIER> | id = <IDENTIFIER>)+ ">"
	{
	  	owl_token[0] = protocol_type;
		owl_token[1] = name_space;
		owl_token[2] = id;
		return owl_token;
	}
}

Token namespace():
{
	Token name_space;
}
{
	name_space = <OWL_NAMESPACE_IDENTIFIER>
		{
		  return name_space;
		}
	| name_space = <SLASH_END_IDENTIFIER>
		{
		  return name_space;
		}
	| name_space = <HASH_END_IDENTIFIER>
		{
		  return name_space;
		}
}

