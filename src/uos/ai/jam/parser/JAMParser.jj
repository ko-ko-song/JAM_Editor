/**********************************************************************/

void agent_components(Interpreter interpreter) :
{
	Plan plan = null;
}
{
	// Allow arbitrary interleaving of Goals, Plans, Facts, and Observer
	(
		<GOAL_DECL>		( goal(interpreter) )*
	|	<FACT_DECL>		( fact(interpreter) )*
	|   <PREFIX_DECL>	( prefix(interpreter) )* // @mod
//	|	<PLAN_START>	( plan(interpreter) )*
	|	<OBSERVER_DECL>
	 	(
	 		{
		 		Plan currentPlan = new Plan();
		 		PlanSequenceConstruct body_elements = null;
	 		}
			"{" body_elements = plan_construct_sequence(currentPlan.getSymbolTable(), interpreter) "}"
			{
				currentPlan.setBody(body_elements);
				interpreter.setObserver(currentPlan);
			}
		)*
	|	<CATCH> globalCatch(interpreter)
	| 	plan = plan(interpreter) { interpreter.getPlanLibrary().add(plan); }
	)+
}

void prefix(Interpreter interpreter) :
{
	Prefix prefix = null;
	Token protocol_type = null;
	Token name = null;
	NameSpace name_space = null;
}
{	  prefix = prefix_owl(null, interpreter) "<" protocol_type = <PROTOCOLTYPE>  name = namespace() ">" ";"
	{
	  	name_space = new NameSpace(protocol_type.image + name.image);

	  	//editor
	  	prefix.setValue(new String(protocol_type.image + name.image));
	  	
		interpreter.getNameSpaceTable().addPrefix(prefix,name_space);
		interpreter.getNameSpaceTable().addPrefix(prefix);
	}
}

void auto_prefix(Interpreter interpreter) :
{
	Prefix auto_prefix = null;
}
{
	auto_prefix = prefix_owl(null,interpreter) ";"
	{

	}
}

Prefix prefix_owl(SymbolTable symbol_table, Interpreter interpreter) :
{
  Token id;
  Token traceToken;
}
{
	traceToken = <PREFIX> id = <IDENTIFIER> ":"
	{
	  Prefix prefix = new Prefix(id.image);
	  prefix.set_fileName(fileName);
	  prefix.set_line(traceToken.beginLine);
	  return prefix;
	}
}


	
