Plan plan(Interpreter interpreter) :
{
	Plan plan = new Plan();
	Relation relation = null;
	GoalAction goalAction = null;
	Token traceToken = null;
}
{
	traceToken = <PLAN_START> ( ( <PLAN_CONCLUDESPEC> relation = relation(plan.getSymbolTable(), interpreter) ) | goalAction = goal_action(plan.getSymbolTable(), interpreter) ) 
	"{"
		plan_components(plan, interpreter)	
	"}"
	{
	    plan.set_fileName(fileName);
	    plan.set_line(traceToken.beginLine);
		plan.setGoalSpecification(goalAction);
		plan.setConcludeSpecification(relation);
		return plan; 
	}
}

