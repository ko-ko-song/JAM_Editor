List<Expression> object_arguments(SymbolTable symbolTable, Interpreter interpreter) :
{
	List<Expression> exprList = new LinkedList<Expression>();
	Expression e = null;
}
{
	"(" [
		e=expression(symbolTable, interpreter)
		{
			exprList.add(e);
		}
		(
			"," e=expression(symbolTable, interpreter)
			{
				exprList.add(e);
			}
		)*
	] ")"
	{
		return exprList;
	}
}

