	public static ErrorInformation parseStringForErrorDetection(Interpreter interpreter, String buf) {
		JAMParser parser;
		if(interpreter == null)
	  		interpreter = new Interpreter();

		parser = new JAMParser(new ByteArrayInputStream(buf.getBytes()));
	
		try {
			interpreter = (Interpreter) parser.ParseJamAgentDefinition(interpreter);
//			System.out.println("parseStringForErrorDetection : JAM string parse successful.");
			return null;
		} catch (ParseException e) {
			ErrorInformation info = new ErrorInformation();
			System.out.println("expected token sequence : ------  " + e.expectedTokenSequences);
			info.setExpectedTokens(e.expectedTokenSequences);
			info.setTokenImage(e.tokenImage);
			info.setCurrentToken(parser.token);
			info.setNextToken(parser.token.next);
			
			return info;
		} catch (Exception e) {
			System.out.println(" ----------------------------- exception");
//			e.printStackTrace();
			return null;
		}
	}

